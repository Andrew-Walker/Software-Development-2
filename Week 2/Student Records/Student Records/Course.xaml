<Window x:Class="Student_Records.Course"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Course" Height="300" Width="300">
    <Grid>
        <Button x:Name="okButton" Content="Ok" HorizontalAlignment="Left" Margin="207,242,0,0" VerticalAlignment="Top" Width="75" Click="okButton_Click"/>
        <ComboBox x:Name="courseComboBox" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" SelectionChanged="courseComboBox_SelectionChanged" Loaded="courseComboBox_Loaded"/>

    </Grid>
</Window>
